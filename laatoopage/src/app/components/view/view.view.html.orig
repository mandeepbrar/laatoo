<div>
	<div class="row" ng-transclude>
	</div>
	<pagination boundary-links="true" ng-change="refreshView()" total-items="totalrecords" items-per-page="pagesize" ng-model="pagenum" class="pagination-sm" previous-text="&lsaquo;" next-text="&rsaquo;" first-text="&laquo;" last-text="&raquo;" ng-show="paginate"></pagination>	
	<div class="row" ng-show="editable">
	  <button type="submit" class="btn btn-default" ng-click="onSubmit()">{{submitText}}</button>
	</div>
</div>	