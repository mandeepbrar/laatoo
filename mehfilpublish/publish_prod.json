{
	"services":{
		"user_storer":{
			"servicename":"mongo_data_service",
			"connectionstring":"localhost",
			"database":"Users",
			"collection":"Users",
			"object":"default_user"
		},
		"auth":{
			"servicename":"auth_service",
			"user_data_svc":"user_storer",
			"path":"/auth"
		},
		"public":{
			"servicename":"static_file_service",
			"publicdir":"publish_prod/static",
			"path":""
		},
		"pageservice":{
			"servicename":"page_service",
			"pagesdir":"publish_prod/pageservice",
			"path":"",
			"pages":{
				"home":{
					"path":"/home",
					"type":"file",
					"destination":"home.html",
					"authentication":"true",
					"permission":"",
					"params": {
					}
				},
				"articles":{
					"path":"/articles",
					"type":"file",
					"destination":"articles.html",
					"authentication":"true",
					"permission":"",
					"params": {
					}
				},
				"login":{
					"path":"/login",
					"type":"file",
					"destination":"login.html",
					"authentication":"true",
					"permission":"",
					"params": {
						"LocalAuthServer":"http://localhost:7070/auth/login",
						"SuccessRedirect":"/articles",
						"AuthToken":"Auth-Token"
					}
				},
				"logout":{
					"path":"/logout",
					"type":"redirect",
					"destination":"/auth/logout",
					"params": {
					}
				}
			}
		}
	}
}