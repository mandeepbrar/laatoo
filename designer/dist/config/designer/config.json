{
  "modules":{
    "filesystemstorage":{
      "module":"filesystemstorage",
      "settings":{
      }
    },
    "mongodatabase": {
      "module":"mongodatabase",
      "settings": {
        "mongoconnectionstring":"127.0.0.1",
        "mongodatabase":"PunjabiMehfil"
      }
    },
    "users": {
      "module":"dataadapter",
      "settings":{
        "object":"User",
        "dataservicefactory":"mongodatabase"
      }
    },
    "staticfileserver":{
      "module":"staticfileserver",
      "settings":{
      }
    },
    "publicdirserver":{
      "module":"publicdirserver",
      "settings":{
        "publicfilesdir":"/bin/publicdir",
        "instance":"publicdir"
      }
    },
    "publicfilesserver":{
      "module":"publicfilesserver",
      "settings":{
        "filestoragedir":"/bin/appfiles",
        "instance":"appfiles",
        "files": {
          "services.conf.js": {
            "cache": "true",
            "path": "scripts/services.conf.js"
          },
          "index.html": {
            "cache": "true",
            "path": "index.html"
          }
        }
      }
    }
  }
}
