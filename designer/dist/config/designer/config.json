{
  "modules":{
    "filesystemstorage":{
      "module":"filesystemstorage",
      "settings":{
      }
    },
    "mongodatabase": {
      "module":"mongodatabase",
      "environment": {
        "mongoconnectionstring":"mongo",
        "mongodatabase":"PunjabiMehfil"
      }
    },
    "roles": {
      "module":"dataadapter",
      "logging": {
        "format":"happymaxcolor",
        "level":"trace"
      },
      "settings":{
        "object":"Role",
        "dataservicefactory":"mongodatabase"
      }
    },
    "staticfileserver":{
      "module":"staticfileserver",
      "settings":{
      }
    },
    "publicdirserver":{
      "module":"publicdirserver",
      "settings":{
        "publicfilesdir":"/bin/publicdir",
        "instance":"publicdir"
      }
    },
    "publicfilesserver":{
      "module":"publicfilesserver",
      "settings":{
        "filestoragedir":"/bin/appfiles",
        "instance":"appfiles",
        "files": {
          "services.conf.js": {
            "cache": "true",
            "path": "scripts/services.conf.js"
          },
          "index.html": {
            "cache": "true",
            "path": "index.html"
          }
        }
      }
    }
  }
}
